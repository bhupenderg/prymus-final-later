                    <input type="text" name="position" class="position" placeholder="Position">
                </div>    
                <div class="general-info-3 general-details">
                    <input type="text" name="city" class="city" placeholder="City">
                    <input type="text" name="state" class="state" placeholder="State">
                    <input type="text" name="country" class="country" placeholder="Country">
                </div>
                <div class="general-info-4 general-details">
                    <!-- <input type="range" min="1" max="100" value="50" class="slider" id="myRange" placeholder="Budget"> -->
                <textarea name="requirement" class="requirement" cols="30" rows="10" placeholder="Requirement"></textarea>
                </div>
                <div class="general-submit">
                    <input type="submit" name="submit" value="Submit" class="general-submit-btn">
                </div>
                
            </form>
        </div>
        
