    <!-- contact heading -->

    <div class="form-contact">
        <div class="form-contact-heading">
            <h1>tell us about your project</h1>
        </div>
        <div class="contact-form">
            <form action="mailto:i.bhupendr@gmail.com" method="POST" enctype="multipart/form-data">

                <div class="general-info-1 general-details">
                    <input type="text" name="name" class="name" placeholder="Name">
